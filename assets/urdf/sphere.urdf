<?xml version="1.0"?>
<robot name="object">
<!-- The OBJ file was created in Blender with a Image Texture. -->
<!-- intended to approximate a gacha ball -->
  <link name="object">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
      	<mesh filename="sphere.obj" scale=".030 .030 .030"/>
        <!--mesh filename="sphere.obj" scale=".024 .024 .024"/-->
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.030"/>
        <!--sphere radius="0.024"/-->
      </geometry>
    </collision>
    <inertial>
      <!-- make total weight of sphere 58 grams, about the same as a tennis ball -->
      <density value="352.0"/>
    </inertial>
  </link>
</robot>
